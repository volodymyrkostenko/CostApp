<%=form_for([@project, @project.items.build]) do |f| %>
  <%=f.collection_select(:category_id, Category.all, :id, :title)%>
  <%=f.label :title%>
  <%=f.text_field :title %>	
  <%=f.label :count%>
  <%=f.text_field :count %>
   <%=f.label :price%>
  <%=f.text_field :price %>
   <%=f.label :total%>
  <%=f.text_field :total %>
  <%= f.submit%>
  
  
<%end%>